{
  "name": "apidef",
  "version": "0.0.1-alpha",
  "description": "Generate api definition files and docs",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "cleanup": "rm -rf dist",
    "push-src": "./push-src.sh",
    "tscheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cli": "cross-env NODE_ENV=development ts-node src/index-cli.ts",
    "depl": "cross-env NODE_ENV=development npm uninstall -g apidef && yarn build && npm install -g && yarn push-src"
  },
  "author": "Ikmich",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/express-list-endpoints": "^4.0.1",
    "@types/fs-extra": "^9.0.6",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.28",
    "@types/yargs": "^15.0.12",
    "conf": "^8.0.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "prettier": "^2.2.1",
    "slugify": "^1.4.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yargs": "^16.2.0"
  },
  "bin": {
    "apidef": "dist/index-cli.js"
  }
